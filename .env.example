# Required
MISTRAL_API_KEY=your_key_here

# Postgres Configuration (optional, defaults shown)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=ai_api_prod

# Vector Configuration (optional)
# Must match the output dimension of the embedding model you obtain vectors from.
# e.g. OpenAI text-embedding-3-small is 1536. Mistral-embed is 1024.
# Important: Changing this after DB initialization requires a migration/reset.
VECTOR_DIMENSION=1536